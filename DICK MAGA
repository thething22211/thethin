local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/sol"))()
local theMob = game.Players.LocalPlayer.Character 

local win = SolarisLib:New({
  Name = "IMS cheats",
  FolderToSave = "SolarisLibStuff"
})

local tab0 = win:Tab("Credets/updats/GoingToAdd")

local tab = win:Tab("Chricter Mods")


local sec = tab:Section("Elements")


sec:Button("Button", function()
  SolarisLib:Notification("Test", "This is a notification test.")
end)


local toggle = sec:Toggle("Toggle", false,"Toggle", function(t)
  print(t)
end)






local slider = sec:Slider("Slider", 0,100,0,1,"Slider", function(t)
  print(t)
end)





local dropdown = sec:Dropdown("Dropdown", {"a","b","c","d","e"},"","Dropdown", function(t)
  print(t)
end)






local multidropdown =sec:MultiDropdown("Multi Dropdown", {"a","b","c","d","e"},{"b", "c"},"Dropdown", function(t)
  print(table.concat(t, ", "))
end)





sec:Colorpicker("Colorpicker", Color3.fromRGB(255,255,255),"Colorpicker", function(t)
  print(t)
end)


sec:Textbox("Textbox", true, function(t)
    SolarisLib:Notification("Test",t)
  print(t)
end)



local label = sec:Label("Label")


local tab2 = win:Tab("Suit Mods")
local sec1 = tab2:Section("Suit Things")
local sec2 = tab2:Section("Call parts")
local sec3 = tab2:Section("Chrome Suit")
local sec4 = tab2:Section("Other scipts i made")
local sec5 = tab2:Section("Light ON/OFF")
--call parts
local dropdown = sec2:Dropdown("call part", {"Mask","Helmet","UpperTorso","LowerTorso","RightHand","RightLowerArm","RightUpperArm","LeftHand","LeftLowerArm","LeftUpperArm","RightUpperLeg","RightLowerLeg","RightFoot","LeftUpperLeg","LeftLowerLeg","LeftFoot"},"","Dropdown", function(t)
    game.Players.LocalPlayer.Backpack.ironMan.Assets.Events.callPiece:FireServer(t)
  end)
--suit things
sec1:Button("Chrom All", function()
    for i,v in pairs(game.Players.LocalPlayer.Character.Suit:GetDescendants()) do
        if v.name == "Union" then
            if v ~= game.Players.LocalPlayer.Character.Suit.Helmet.Union then
            v:Destroy()
            end
        end
    end
    game.Players.LocalPlayer.Character.Suit.RightUpperArm.Part:Destroy()
    game.Players.LocalPlayer.Character.Suit.LeftUpperArm.Part:Destroy()
  end)

--suit things
  sec1:Button("Strong Blast",function ()
    local count = 0
	while count < 25 do
	game.Players.LocalPlayer.Backpack.suitControl.Client:clone().Parent = game.Players.LocalPlayer.Backpack.suitControl
	count = count +1
	end
end)
--chrome suit

sec3:Button("Right sholder",function ()
    game.Players.LocalPlayer.Character.Suit.RightUpperArm.Union:Destroy()
    game.Players.LocalPlayer.Character.Suit.RightUpperArm.Union:Destroy()
    game.Players.LocalPlayer.Character.Suit.RightUpperArm.Union:Destroy()
    game.Players.LocalPlayer.Character.Suit.RightUpperArm.Part:Destroy()
end)


sec3:Button("Torso",function ()
    theMob.Suit.LowerTorso.Union:Destroy()
    theMob.Suit.LowerTorso.Union:Destroy()  

    theMob.Suit.UpperTorso.Union:Destroy()
    theMob.Suit.UpperTorso.Union:Destroy()
    theMob.Suit.UpperTorso.Union:Destroy()
    theMob.Suit.UpperTorso.Union:Destroy()
    theMob.Suit.UpperTorso.Union:Destroy()
    theMob.Suit.UpperTorso.Union:Destroy()
end)


